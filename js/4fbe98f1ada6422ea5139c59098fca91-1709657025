    setTimeout(function () {

        function preloadImage(elements) {

            $.each(elements, function (index, element) {
                var image = $(element).data("image");
                if (image.length) {
                    (new Image()).src = image;
                }
            });

        }

        $(".animation").waypoint(function () {
            $(this.element).addClass($(this.element).data("animation"));
            //			$(this.element).on("animationend", function() {
            //				$(this).removeClass('animated');
            //			});

            this.destroy();

        }, {
            offset: '100%'
        });

        $(".success-comp-logos").waypoint(function () {
            // preloadImage($("a", this.element));
            this.destroy();
        }, {
            offset: '100%'
        });

        $(".animation-end").on("animationend", function () {
            var animationend = $(this).data("animationend");
            var animationstart = $(this).data("animationstart");
            $(animationstart).addClass(animationend);
        });

    }());

    setTimeout(function () {
        $(".carousel").on("slide.bs.carousel", function (ev) {
            var lazy = $(ev.relatedTarget).find("img[data-src]");
            lazy.attr("src", lazy.data('src'));
            lazy.removeAttr("data-src");
        });
    });

    setTimeout(function () {

        $(".swiper-gallery-full").each(function (index, elm) {

            var swiper, scrollInterval, scrollPosition, virtualSize, scrollIntervalTimer, scrollSize, translateDuration;

            scrollIntervalTimer = 150;
            translateDuration = 500;
            scrollSize = 150;

            swiper = new Swiper(elm, {
                // freeMode: true,
                // mousewheel: true,
                // slidesPerView: 'auto',
                // spaceBetween: 30,
                // centeredSlides: true,
                scrollbar: {
                    el: '.swiper-scrollbar',
                    hide: false,
                    draggable: true,
                    dragSize: 160,
                },
                // keyboard: {
                // 	enabled: true
                // },
                slidesPerView: 'auto',
                spaceBetween: 30,
                freeMode: true,
                mousewheel: {
                    forceToAxis: true,
                    invert: true,
                    sensitivity: 1,
                },
                breakpoints: {
                    640: {
                        spaceBetween: 30
                    },
                    320: {
                        spaceBetween: 0
                    }
                }
            });

            $("img", elm).on("load", function () {
                if (swiper) {
                    swiper.update();
                }
            });

            $("img", elm).each(function (idx, img) {
                $(this).attr('src', $(this).attr('src'));
            });

            $(".right-invisible-arrow", elm).hover(function () {
                scrollPosition = swiper.translate;
                virtualSize = swiper.virtualSize;

                scrollInterval = setInterval(function () {
                    if (-(virtualSize - window.innerWidth) <= scrollPosition) {
                        scrollPosition = scrollPosition - scrollSize;
                    } else {
                        scrollPosition = -(virtualSize - window.innerWidth);
                    }
                    swiper.translateTo(scrollPosition, translateDuration, true, true);
                }, scrollIntervalTimer);
            }, function () {
                clearInterval(scrollInterval);
            });

            $(".left-invisible-arrow", elm).hover(function () {
                scrollPosition = swiper.translate;
                virtualSize = swiper.virtualSize;

                scrollInterval = setInterval(function () {
                    if (scrollPosition < 0) {
                        scrollPosition = scrollPosition + scrollSize;
                    } else {
                        scrollPosition = 0;
                    }
                    swiper.translateTo(scrollPosition, translateDuration, true, true);
                }, scrollIntervalTimer);
            }, function () {
                clearInterval(scrollInterval);
            });

        });

        // window._swiper = swiper;

    });

    // Process photo length check for arrow
    if ($("#carouselIndicators0").length > 0) {
        var processCarouselCount = document.querySelectorAll('.carousel-inner').length;
        for (var i = 0; i < processCarouselCount; i++) {
            if ($("#carouselIndicators" + i + " .carousel-inner")[0].childElementCount === 1) {
                $("#" + i + ".process-invisible-right").addClass("one-photo");
                $("#" + i + ".process-invisible-left").addClass("one-photo");
            }
        }
    }

    let video_iframe = $(".video-wrapper iframe.wrapper-inter-video");

    if (video_iframe.length) {
        let video_height = video_iframe.contents().find("video").height();
        video_iframe.css('height', video_height);
    }

    let contact_form = $('.contacts-form');
    if (contact_form.length) {
        let countries_list = $('.contacts-form .countries');

        $('.contacts-form .selected-country').on('click', function (e) {
            countries_list.toggle();
            countries_list.scrollTop(0);

            if (countries_list.is(":visible")) {
                $('html').one('click', function () {
                    countries_list.hide();
                });

                $(document).keypress(function (e) {
                    countries_list.find('.country').each(function (index) {
                        if ($(this).find('.name').text()[0].toUpperCase() === String.fromCharCode(e.which).toUpperCase()) {
                            countries_list.scrollTop(index * $(this).outerHeight());
                            return false;
                        }
                    });
                });
            }

            e.stopPropagation();
        });

        let countries_list_country = $('.contacts-form .countries .country');
        countries_list_country.on('click', function () {
            countries_list_country.removeClass('active');
            $(this).addClass('active');

            let code = $(this).attr('data-country-code');
            let src = $(this).find('.flag').attr('src');
            let name = $(this).find('.flag').attr('alt');
            let phone_code = $(this).find('.code').text();

            setCountry(code, src, name, phone_code);
        });

        $.ajax({
            url: 'https://api.country.is/',
            success: function (data) {
                let selected_country = $('.contacts-form .countries .country[data-country-code="' + data.country + '"]');
                if (selected_country.length) {
                    countries_list_country.removeClass('active');
                    selected_country.addClass('active');

                    let code = selected_country.attr('data-country-code');
                    let src = selected_country.find('.flag').attr('src');
                    let name = selected_country.find('.flag').attr('alt');
                    let phone_code = selected_country.find('.code').text();

                    setCountry(code, src, name, phone_code);
                }
            }
        });
    }

    function setCountry(code, src, name, phone_code) {
        let active_country = $('.selected-country');

        active_country.find('.flag').attr('src', src);
        active_country.find('.flag').attr('alt', name);
        active_country.find('.code').text(phone_code);

        let phone_code_select = $('.contacts-form #phone_code');
        let selected_phone_code = phone_code_select.find('option[data-country-code="' + code + '"]');

        if (selected_phone_code.length) {
            $('.contacts-form .phone_code').prop("selected", false);
            selected_phone_code.prop("selected", true);
        }
    }

    // $('.article-content').append(
    //     $(document.createElement('button')).prop({
    //         type: 'button',
    //         innerHTML: 'Play video',
    //         class: 'fake-button'
    //     })
    // );
    //
    // let fake_button = $('.fake-button');

    let self_blog_video = $(".article-content video");
    if (self_blog_video.length > 0) {
        self_blog_video.each(function (i, obj) {
            let video_url = $(this).attr("src");
            // let video = $('<video />', {
            //     id: 'video',
            //     src: video_url,
            //     type: 'video/mp4',
            //     controls: false,
            //     autoplay: true,
            //     loop: true,
            //     muted: true,
            //     playsInline: true
            // });

            let video = document.createElement('video');
            video.controls = false;
            video.autoPlay = true;
            video.muted = true;
            video.loop = true;
            video.playsInline = true;
            $(this).replaceWith(video);

            addSourceToVideo(video, video_url, 'video/mp4');

            video.play();
        });
    }

    function addSourceToVideo(element, src, type) {
        let source = document.createElement('source');

        source.src = src;
        source.type = type;

        element.appendChild(source);
    }

    let left = (($(window).width() - 1136) / 2);

    let initialScrollPositionOurAwards = 0;
    let scrollPositionOurAwards = 0;
    let singleElWidthOurAwards = 0;
    let slidesCountOurAwards = 0;
    if ($('#owlOurAwards').length) {
        $("#owlOurAwards").owlCarousel({
            items: 8,
            lazyLoad: false,
            lazyLoadEager: 100,
            dots: false,
            loop: true,
            mouseDrag: false,
            center: false,
            mergeFit: false,

            responsive: {
                0: {
                    items: 7,
                    stagePadding: left,
                },
                636: {
                    items: 5,
                    stagePadding: left
                },
                1136: {
                    items: 6,
                    stagePadding: left
                }
            },
            onChanged: function (event) {
                if (event.item.count) {
                    $("#owlOurAwards a").removeClass("active");
                    $("#owlOurAwards a:eq(" + event.item.index + ")").addClass("active");
                }
            },
        });

        let slideOwlOurAwardsInterval;
        let allElWidthOurAwards = 0;
        let summMarginOurAwards = 0;
        slidesCountOurAwards = $("#owlOurAwards").data('count');
        let firstElementIndexOurAwards = null;
        $("#owlOurAwards .owl-stage .owl-item").each((index, element) => {
            singleElWidthOurAwards = element.offsetWidth;
            allElWidthOurAwards += element.offsetWidth;
            summMarginOurAwards += 16;

            if (firstElementIndexOurAwards === null && $(element).find('a').data('slide') === 'slide-0') {
                firstElementIndexOurAwards = index;
                initialScrollPositionOurAwards = -singleElWidthOurAwards * index;
                $("#owlOurAwards .owl-stage").css("transform", "translate3d(" + initialScrollPositionOurAwards + "px, 0px, 0px)");
                scrollPositionOurAwards = initialScrollPositionOurAwards;
            }
        });
        $("#owlOurAwards .owl-stage").css("width", (allElWidthOurAwards + summMarginOurAwards) + "px");

        $('.our-awards-arrow-right').on('mouseenter', function (e) {
            slideOwlOurAwards('next', 1000);
        })
        $('.our-awards-arrow-right').on('mouseleave', function (e) {
            clearInterval(slideOwlOurAwardsInterval);
        })

        $('.our-awards-arrow-left').on('mouseenter', function (e) {
            slideOwlOurAwards('prev', 1000);
        })
        $('.our-awards-arrow-left').on('mouseleave', function (e) {
            clearInterval(slideOwlOurAwardsInterval);
        })

        function slideOwlOurAwards(action, millisecond) {
            if (activeLeftRightSlideOurAwards) {
                scrollPositionOurAwards = parseFloat($('#owlOurAwards .owl-stage').css('transform').split(',')[4]);

                slideOwlOurAwardsInterval = setInterval(function () {

                    $("#owlOurAwards .owl-stage").css("transform", "translate3d(" + scrollPositionOurAwards + "px, 0px, 0px)");

                    if (action === 'next') {
                        if (initialScrollPositionOurAwards - (singleElWidthOurAwards * (slidesCountOurAwards + 5)) + window.innerWidth < scrollPositionOurAwards) {
                            scrollPositionOurAwards = scrollPositionOurAwards - 1;
                        } else {
                            scrollPositionOurAwards = scrollPositionOurAwards + singleElWidthOurAwards * slidesCountOurAwards;
                        }
                    } else if (action === 'prev') {
                        if (initialScrollPositionOurAwards + (singleElWidthOurAwards * 5) > scrollPositionOurAwards) {
                            scrollPositionOurAwards = scrollPositionOurAwards + 1;
                        } else {
                            scrollPositionOurAwards = scrollPositionOurAwards - singleElWidthOurAwards * slidesCountOurAwards;
                        }
                    }
                }, 2);
            }
        }
    }

    let initialScrollPositionTrustedBy = 0;
    let scrollPositionTrustedBy = 0;
    let singleElWidthTrustedBy = 0;
    let slidesCountTrustedBy = 0;
    if ($('#owlTrustedBy').length) {
        $("#owlTrustedBy").owlCarousel({
            items: 8,
            lazyLoad: false,
            lazyLoadEager: 100,
            dots: false,
            loop: true,
            mouseDrag: false,
            center: false,
            mergeFit: false,

            responsive: {
                0: {
                    items: 7,
                    stagePadding: left,
                },
                636: {
                    items: 5,
                    stagePadding: left,
                },
                1136: {
                    items: 6,
                    stagePadding: left
                }
            },
            onChanged: function (event) {
                if (event.item.count) {
                    $("#owlTrustedBy a").removeClass("active");
                    $("#owlTrustedBy a:eq(" + event.item.index + ")").addClass("active");
                }
            }
        });

        let slideOwlTrustedByInterval;
        let allElWidthTrustedBy = 0;
        let summMarginTrustedBy = 0;
        slidesCountTrustedBy = $("#owlTrustedBy").data('count');
        let firstElementIndexTrustedBy = null;
        $("#owlTrustedBy .owl-stage .owl-item").each((index, element) => {
            singleElWidthTrustedBy = element.offsetWidth;
            allElWidthTrustedBy += element.offsetWidth;
            summMarginTrustedBy += 16;

            if (firstElementIndexTrustedBy === null && $(element).find('a').data('slide') === 'slide-0') {
                firstElementIndexTrustedBy = index;
                initialScrollPositionTrustedBy = -singleElWidthTrustedBy * index;
                $("#owlTrustedBy .owl-stage").css("transform", "translate3d(" + initialScrollPositionTrustedBy + "px, 0px, 0px)");
                scrollPositionTrustedBy = initialScrollPositionTrustedBy;
            }
        });
        $("#owlTrustedBy .owl-stage").css("width", (allElWidthTrustedBy + summMarginTrustedBy) + "px");

        $('.trusted-by-arrow-right').on('mouseenter', function (e) {
            slideOwlTrustedBy('next', 1000);
        })
        $('.trusted-by-arrow-right').on('mouseleave', function (e) {
            clearInterval(slideOwlTrustedByInterval);
        })

        $('.trusted-by-arrow-left').on('mouseenter', function (e) {
            slideOwlTrustedBy('prev', 1000);
        })
        $('.trusted-by-arrow-left').on('mouseleave', function (e) {
            clearInterval(slideOwlTrustedByInterval);
        })

        function slideOwlTrustedBy(action, millisecond) {
            if (activeLeftRightSlideTrustedBy) {
                scrollPositionTrustedBy = parseFloat($('#owlTrustedBy .owl-stage').css('transform').split(',')[4]);

                slideOwlTrustedByInterval = setInterval(function () {
                    $("#owlTrustedBy .owl-stage").css("transform", "translate3d(" + scrollPositionTrustedBy + "px, 0px, 0px)");

                    if (action === 'next') {
                        if (initialScrollPositionTrustedBy - (singleElWidthTrustedBy * (slidesCountTrustedBy + 5)) + window.innerWidth < scrollPositionTrustedBy) {
                            scrollPositionTrustedBy = scrollPositionTrustedBy - 1;
                        } else {
                            scrollPositionTrustedBy = scrollPositionTrustedBy + singleElWidthTrustedBy * slidesCountTrustedBy;
                        }
                    } else if (action === 'prev') {
                        if (initialScrollPositionTrustedBy + (singleElWidthTrustedBy * 5) > scrollPositionTrustedBy) {
                            scrollPositionTrustedBy = scrollPositionTrustedBy + 1;
                        } else {
                            scrollPositionTrustedBy = scrollPositionTrustedBy - singleElWidthTrustedBy * slidesCountTrustedBy;
                        }
                    }
                }, 2);
            }
        }
    }

    let dragItemOurAwards = $("#owlOurAwards .owl-stage");
    let dragItemTrustedBy = $("#owlTrustedBy .owl-stage");

    let activeLeftRightSlideOurAwards = true;
    let activeLeftRightSlideTrustedBy = true;

    let activeOurAwards = false;
    let activeTrustedBy = false;
    let currentXOurAwards;
    let currentYOurAwards;
    let currentXTrustedBy;
    let currentYTrustedBy;
    let initialXOurAwards;
    let initialYOurAwards;
    let initialXTrustedBy;
    let initialYTrustedBy;
    let swipeLocked = false;
    let scrollLocked = false;

    document.body.addEventListener("mousedown", dragStart, false);
    document.body.addEventListener("mouseup", dragEnd, false);
    document.body.addEventListener("mousemove", drag, false);
    document.addEventListener('touchstart', touchStart, false);
    document.addEventListener('touchend', touchEnd, false);
    document.addEventListener('touchmove', touchDrag, false);

    function dragStart(e) {
        if (e.target.closest('.owl-stage') === dragItemOurAwards[0]) {
            e.preventDefault();

            activeLeftRightSlideOurAwards = false;
            initialXOurAwards = e.clientX;
            activeOurAwards = true;
        } else if (e.target.closest('.owl-stage') === dragItemTrustedBy[0]) {
            e.preventDefault();

            activeLeftRightSlideTrustedBy = false;
            initialXTrustedBy = e.clientX;
            activeTrustedBy = true;
        }
    }

    function touchStart(e) {
        if (e.target.closest('.owl-stage') === dragItemOurAwards[0]) {
            initialXOurAwards = e.touches[0].clientX;
            initialYOurAwards = e.touches[0].clientY;

            activeOurAwards = true;
        } else if (e.target.closest('.owl-stage') === dragItemTrustedBy[0]) {
            initialXTrustedBy = e.touches[0].clientX;
            initialYTrustedBy = e.touches[0].clientY;

            activeTrustedBy = true;
        }
    }

    function dragEnd(e) {
        if (activeOurAwards) {
            activeOurAwards = false;

            $("#owlOurAwards .owl-stage .owl-item a").removeClass('dragging');

            scrollPositionOurAwards = scrollPositionOurAwards + currentXOurAwards;
            $("#owlOurAwards .owl-stage").css("transform", "translate3d(" + scrollPositionOurAwards + "px, 0px, 0px)");

            setTimeout(function () {
                activeLeftRightSlideOurAwards = true;
                $('.our-awards-arrow-left').removeClass('disabled');
                $('.our-awards-arrow-right').removeClass('disabled');
            }, 2000);

            document.body.style.overflowY = "auto";
        } else if (activeTrustedBy) {
            activeTrustedBy = false;


            $("#owlTrustedBy .owl-stage .owl-item a").removeClass('dragging');

            scrollPositionTrustedBy = scrollPositionTrustedBy + currentXTrustedBy;
            $("#owlTrustedBy .owl-stage").css("transform", "translate3d(" + scrollPositionTrustedBy + "px, 0px, 0px)");

            setTimeout(function () {
                activeLeftRightSlideTrustedBy = true;
                $('.trusted-by-arrow-left').removeClass('disabled');
                $('.trusted-by-arrow-right').removeClass('disabled');
            }, 2000);

            document.body.style.overflowY = "auto";
        }
    }

    function touchEnd(e) {
        if (activeOurAwards) {
            activeOurAwards = false;
            enableScroll();
        } else if (activeTrustedBy) {
            activeTrustedBy = false;
            enableScroll();
        }

        swipeLocked = false;
        scrollLocked = false;
    }

    function drag(e) {
        if (activeOurAwards) {
            $('.our-awards-arrow-left').addClass('disabled');
            $('.our-awards-arrow-right').addClass('disabled');

            if (e.target.tagName.toLowerCase() === 'a') {
                $(e.target).addClass("dragging");
            } else if (e.target.tagName.toLowerCase() === 'img') {
                $(e.target).parent().addClass("dragging");
            }

            currentXOurAwards = e.clientX - initialXOurAwards;

            if (initialScrollPositionOurAwards - (singleElWidthOurAwards * (slidesCountOurAwards + 5)) + window.innerWidth >= scrollPositionOurAwards) {
                scrollPositionOurAwards = scrollPositionOurAwards + singleElWidthOurAwards * slidesCountOurAwards;

                initialXOurAwards = e.clientX;
            } else if (initialScrollPositionOurAwards + (singleElWidthOurAwards * 5) <= scrollPositionOurAwards) {
                scrollPositionOurAwards = scrollPositionOurAwards - singleElWidthOurAwards * slidesCountOurAwards;

                initialXOurAwards = e.clientX;
            } else {
                $("#owlOurAwards .owl-stage").css("transform", "translate3d(" + (scrollPositionOurAwards + currentXOurAwards) + "px, 0px, 0px)");
            }
        } else if (activeTrustedBy) {
            $('.trusted-by-arrow-left').addClass('disabled');
            $('.trusted-by-arrow-right').addClass('disabled');

            currentXTrustedBy = e.clientX - initialXTrustedBy;

            if (e.target.tagName.toLowerCase() === 'a') {
                $(e.target).addClass("dragging");
            } else if (e.target.tagName.toLowerCase() === 'img') {
                $(e.target).parent().addClass("dragging");
            }

            if (initialScrollPositionTrustedBy - (singleElWidthTrustedBy * (slidesCountTrustedBy + 5)) + window.innerWidth >= scrollPositionTrustedBy) {
                scrollPositionTrustedBy = scrollPositionTrustedBy + singleElWidthTrustedBy * slidesCountTrustedBy;

                initialXTrustedBy = e.clientX;
            } else if (initialScrollPositionTrustedBy + (singleElWidthTrustedBy * 5) <= scrollPositionTrustedBy) {
                scrollPositionTrustedBy = scrollPositionTrustedBy - singleElWidthTrustedBy * slidesCountTrustedBy;

                initialXTrustedBy = e.clientX;
            } else {
                $("#owlTrustedBy .owl-stage").css("transform", "translate3d(" + (scrollPositionTrustedBy + currentXTrustedBy) + "px, 0px, 0px)");
            }
        }
    }

    function touchDrag(e) {
        if (activeOurAwards) {
            currentXOurAwards = e.touches[0].clientX - initialXOurAwards;
            currentYOurAwards = e.touches[0].clientY - initialYOurAwards;

            if (!swipeLocked && !scrollLocked) {
                if (currentXOurAwards > 1 || currentXOurAwards < -1) {
                    disableScroll();

                    if (e.target.tagName.toLowerCase() === 'a') {
                        $(e.target).addClass("dragging");
                    } else if (e.target.tagName.toLowerCase() === 'img') {
                        $(e.target).parent().addClass("dragging");
                    }

                    swipeLocked = true;
                    scrollLocked = false;
                } else if (currentYOurAwards > 1 || currentYOurAwards < -1) {
                    swipeLocked = false;
                    scrollLocked = true;
                }
            }
        } else if (activeTrustedBy) {
            currentXTrustedBy = e.touches[0].clientX - initialXTrustedBy;
            currentYTrustedBy = e.touches[0].clientY - initialYTrustedBy;

            if (!swipeLocked && !scrollLocked) {
                if (currentXTrustedBy > 1 || currentXTrustedBy < -1) {
                    disableScroll();

                    if (e.target.tagName.toLowerCase() === 'a') {
                        $(e.target).addClass("dragging");
                    } else if (e.target.tagName.toLowerCase() === 'img') {
                        $(e.target).parent().addClass("dragging");
                    }

                    swipeLocked = true;
                    scrollLocked = false;
                } else if (currentYTrustedBy > 1 || currentYTrustedBy < -1) {
                    swipeLocked = false;
                    scrollLocked = true;
                }
            }
        }
    }

    function disableScroll() {
        // Get the current page scroll position
        let scrollTop = window.scrollY || document.documentElement.scrollTop;
        let scrollLeft = window.scrollX || document.documentElement.scrollLeft;

        // if any scroll is attempted,
        // set this to the previous value
        window.onscroll = function () {
            window.scrollTo(scrollLeft, scrollTop);
        };
    }

    function enableScroll() {
        window.onscroll = function () { };
    }
});

