        var owl = $("#ourValuesTabContent").owlCarousel({
            items: 1,
            dots: false,
            animateOut: 'slideInRightValuesItemsOut',
            animateIn: 'slideInRightValuesItemsIn',
            responsive: {
                0: {
                    dots: true,
                    mouseDrag: true
                },
                636: {
                    dots: false,
                    mouseDrag: false
                },
                1136: {
                    dots: false,
                    mouseDrag: false
                }
            }
        });

        $("#ourValuesTab a").on("click", function (event) {

            var index = $(this).data("index");

            owl.trigger('to.owl.carousel', [index]);

        });

        $(".value-invisible-right").on("click", function () {
            $("#ourValuesTab a.active").closest("li").next("li").find("a").click();
        })
